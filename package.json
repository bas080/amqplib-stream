{
  "name": "amqplib-stream",
  "version": "0.0.2",
  "description": "Node Streams interface for RabbitMQ",
  "main": "./lib/",
  "dependencies": {
    "amqplib": "^0.7.1"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "eslint": "^4.19.1",
    "markdown-toc": "^1.2.0",
    "pre-commit": "^1.2.2",
    "tap": "^12.0.1"
  },
  "scripts": {
    "test": "tap ./lib/*.spec.js",
